{
  "name": "koca-front",
  "description": "koca front",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "svg": "vsvg -s ./src/assets/svg_icons -t ./src/assets/icons --ext ts --es6",
    "serve": "vue-cli-service serve",
    "serve.prod": "vue-cli-service serve --mode production",
    "build": "vue-cli-service build --report",
    "dll": "webpack --progress --config ./build/webpack.dll.config.js",
    "lint": "vue-cli-service lint",
    "build-module": "node ./build/module_build.js",
    "test": "vue-cli-service test:unit",
    "mock.start": "node ./mock/start",
    "mock.install": "node ./mock/install",
    "mock.stop": "node ./mock/stop",
    "prebuild": "npm run dll",
    "check-icon": "node ./build/check_icon.js",
    "approutes": "node build/projects_routes.js --app",
    "approutes.admin-basic": "node build/projects_routes.js --app admin-basic /views/",
    "serve.main": "cross-env VUE_APP_SPA=qiankun VUE_CLI_SERVICE_CONFIG_PATH=./vue.main.config.js vue-cli-service serve",
    "build.main": "cross-env VUE_APP_SPA=qiankun VUE_CLI_SERVICE_CONFIG_PATH=./vue.main.config.js vue-cli-service build --report",
    "serve.admin-basic": "cross-env VUE_APP_NAME=admin-basic VUE_APP_IP=localhost VUE_APP_PORT=7001 VUE_CLI_SERVICE_CONFIG_PATH=./vue.app.config.js vue-cli-service serve",
    "build.admin-basic": "cross-env VUE_APP_NAME=admin-basic VUE_APP_IP=localhost VUE_APP_PORT=7001 VUE_CLI_SERVICE_CONFIG_PATH=./vue.app.config.js vue-cli-service build --report",
    "build.app": "VUE_CLI_SERVICE_CONFIG_PATH=./vue.app.config.js vue-cli-service build --report",
    "gen:licenses": "node ./build/license-checker.js"
  },
